{% extends "./layouts/master.html.j2" %}

{% block title %}{% endblock %}

{% block content %}
<main>
    <div class="main-container">
        <div class="cards-container">
            {% for s in series %}
            <div class="card">
                <div class="card-header-info">
                    <div class="card-header">
                        <a class="card-header-anchor" href="/series/{{ s.series }}">
                            <span class="card-title">{{ s.series }}</span>
                        </a>
                    </div>
                </div>
            </div>
            {% endfor %}
        </div>
    </div>
</main>
{% endblock %}